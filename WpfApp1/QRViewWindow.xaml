<Window x:Class="InventoryManagement.QRViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InventoryManagement"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Title="QRViewWindow" Height="430" Width="400">
    <Grid>
        <Image x:Name="QRImage" HorizontalAlignment="Left" Height="307" Margin="30,30,30,0" VerticalAlignment="Top" Width="336"/>
        <Button x:Name="PrintButton" Content="Print" HorizontalAlignment="Left" Margin="291,355,0,0" VerticalAlignment="Top" Width="75" Click="PrintButton_Click"/>
        <Button x:Name="SaveButton" Content="Save" HorizontalAlignment="Left" Margin="195,355,0,0" VerticalAlignment="Top" Width="75" Click="SaveButton_Click"/>
    </Grid>
</Window>
